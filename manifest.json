{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.1",
  "description": "Organize and search your Chrome tabs efficiently",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
